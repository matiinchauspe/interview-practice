#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/matias/Documents/Development/challenges/interview-challenges/silver.dev-fork/frontend/node_modules/semver/bin/node_modules:/Users/matias/Documents/Development/challenges/interview-challenges/silver.dev-fork/frontend/node_modules/semver/node_modules:/Users/matias/Documents/Development/challenges/interview-challenges/silver.dev-fork/frontend/node_modules:/Users/matias/Documents/Development/challenges/interview-challenges/silver.dev-fork/node_modules:/Users/matias/Documents/Development/challenges/interview-challenges/node_modules:/Users/matias/Documents/Development/challenges/node_modules:/Users/matias/Documents/Development/node_modules:/Users/matias/Documents/node_modules:/Users/matias/node_modules:/Users/node_modules:/node_modules:/Users/matias/Documents/Development/challenges/interview-challenges/silver.dev-fork/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/matias/Documents/Development/challenges/interview-challenges/silver.dev-fork/frontend/node_modules/semver/bin/node_modules:/Users/matias/Documents/Development/challenges/interview-challenges/silver.dev-fork/frontend/node_modules/semver/node_modules:/Users/matias/Documents/Development/challenges/interview-challenges/silver.dev-fork/frontend/node_modules:/Users/matias/Documents/Development/challenges/interview-challenges/silver.dev-fork/node_modules:/Users/matias/Documents/Development/challenges/interview-challenges/node_modules:/Users/matias/Documents/Development/challenges/node_modules:/Users/matias/Documents/Development/node_modules:/Users/matias/Documents/node_modules:/Users/matias/node_modules:/Users/node_modules:/node_modules:/Users/matias/Documents/Development/challenges/interview-challenges/silver.dev-fork/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../semver/bin/semver.js" "$@"
else
  exec node  "$basedir/../semver/bin/semver.js" "$@"
fi
